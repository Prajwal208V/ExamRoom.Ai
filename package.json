{
  "name": "ExamRoom",
  "version": "0.0.1",
  "private": true,
  "description": "ExamRoom by Prajwal V",
  "main": "index.js",
  "scripts": {
    "start": "npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios --simulator=\"iPhone 14 Pro Max\"",
    "clear-watchman": "watchman watch-del-all",
    "reset": "rm -rf node_modules && yarn install && rm -rf /tmp/metro-* && rm -rf ios/Pods ios/Podfile.lock && cd android && ./gradlew clean && npx browserslist@latest --update-db",
    "reset:android": "rm -rf node_modules && yarn install && rm -rf /tmp/metro-* && cd android && ./gradlew clean",
    "build:android": "patch-package && cd android && ./gradlew clean bundleRelease",
    "build:apk": "patch-package && cd android && ./gradlew clean assembleRelease",
    "android:deploy": "yarn run reset:android && yarn run build:android",
    "android:deployAPK": "yarn run reset:android && yarn run build:apk",
    "reset:ios": "rm -rf node_modules && yarn install && rm -rf /tmp/metro-*",
    "pod:clean": "rm -rf ios/Pods ios/Podfile.lock",
    "pod:install": "npx pod-install",
    "ios:deploy": "yarn reset:ios && yarn pod:clean && yarn pod:install && yarn clean:ios",
    "ios:release": "yarn run reset && yarn run pod:clean && yarn run pod:install",
    "test": "jest",
    "lint": "eslint .",
    "docs:build": "jsdoc -c jsdoc.conf.js",
    "docs:dev": "nodemon --watch src --exec \"yarn run docs:build\"",
    "docs": "yarn run docs:build && npx open-cli ./docs/index.html",
    "check-dependencies": "rnx-dep-check",
    "fix-dependencies": "rnx-dep-check --write"
  },
  "dependencies": {
    "@bugsnag/react-native": "^7.20.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-native-fontawesome": "^0.3.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "crypto-js": "3.3.0",
    "formik": "^2.4.2",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-native": "^0.71.11",
    "react-native-blob-util": "^0.18.3",
    "react-native-bootsplash": "^4.7.2",
    "react-native-calendar-picker": "^7.1.4",
    "react-native-device-info": "^10.6.0",
    "react-native-elements": "^3.4.3",
    "react-native-event-listeners": "^1.0.7",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-image-pan-zoom": "^2.1.12",
    "react-native-mmkv": "^2.8.0",
    "react-native-mmkv-flipper-plugin": "^1.0.0",
    "react-native-modal": "^13.0.1",
    "react-native-modalbox": "^2.0.2",
    "react-native-paper": "4.12.4",
    "react-native-pdf": "^6.6.2",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.21.0",
    "react-native-simple-toast": "^2.0.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-step-indicator": "^1.0.3",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^9.2.0",
    "react-navigation": "^4.4.4",
    "react-redux": "^8.1.0",
    "redux-batched-actions": "^0.5.0",
    "redux-persist": "^6.0.0",
    "victory-native": "^36.6.11",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.8.8",
    "react-native-flipper": "^0.198.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
